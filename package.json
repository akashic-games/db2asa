{
  "name": "@akashic-extension/db2asa",
  "version": "1.0.5",
  "description": "Convert DragonBones for akashic-animation",
  "bin": {
    "db2asa": "bin/index.js"
  },
  "scripts": {
    "prepublish": "npm run install:typings && gulp compile",
    "install:typings": "gulp install:typings",
    "build": "gulp compile",
    "clean:typings": "gulp clean:typings",
    "clean": "gulp clean",
	"test": "npm run test:jasmine && npm run lint",
	"test:jasmine": "istanbul cover --report text --report html --colors -i ./lib/index.js ./node_modules/jasmine/bin/jasmine.js",
    "lint": "tslint -c tslint.json --project ./tsconfig.json"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/db2asa.git"
  },
  "publishConfig": {
    "@akashic-extension:registry": "https://registry.npmjs.org/"
  },
  "files": [
    "README.md",
    "package.json",
    "lib",
    "bin"
  ],
  "devDependencies": {
    "del": "^2.2.2",
    "gulp": "^4.0.0",
    "gulp-istanbul": "^1.1.3",
    "gulp-jasmine": "^2.2.1",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-shell": "^0.5.2",
    "gulp-tslint": "^4.2.1",
    "jasmine": "^2.4.1",
    "jasmine-reporters": "^2.0.7",
    "jasmine-terminal-reporter": "^1.0.2",
    "mdast": "^2.3.2",
    "mdast-lint": "~1.1.1",
    "mock-fs": "^3.5.0",
    "tslint": "^3.5.0",
    "typescript": "2.9.2",
    "typings": "2.1.1"
  },
  "dependencies": {
    "@akashic-extension/akashic-animation": "^2.6.1",
    "@akashic/akashic-engine": "~1.10.0",
    "commander": "^2.9.0",
    "fs-extra": "^0.26.2",
    "image-size": "^0.5.0",
    "recursive-readdir": "^2.1.0"
  }
}
